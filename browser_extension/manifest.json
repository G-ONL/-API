{
  "manifest_version": 2,

  "name": "Hackday_Extension",
  "description": "Hackday Payment Extension",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "storage",
    "https://www.gitflow.org/*",
    "http://localhost:8080/*"
  ]
}